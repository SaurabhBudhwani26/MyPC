{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types.d.ts",
    "react-native-override.d.ts",
    "react-types-fix.d.ts",
    "nativewind-env.d.ts"
  ]
}