{
  "expo": {
    "name": "MyPC",
    "slug": "mypc-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "mypc",
    "description": "Compare PC component prices and build your perfect gaming or workstation PC with MyPC.",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saurabhbudhwani.mypc",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saurabhbudhwani.mypc",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "owner": "saurabh_budhwani",
    "extra": {
      "eas": {
        "projectId": "84233797-f255-4164-99ca-5a919c0313df"
      }
    }
  }
}
